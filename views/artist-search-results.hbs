<div id="eachArtist">
  {{#each searchResults}}
    <div id="artist">
      <h2>{{this.name}}</h2>
      <img src="{{this.images.1.url}}" alt="imagen" width="300px" height="300px" />
      <div id="button-albums""><a href="albums/{{this.id}}"><button>View Albums</button></a></div>
    </div>
  {{/each}}

</div>